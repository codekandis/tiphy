{
	"name": "codekandis/tiphy",
	"description": "This library is a tiny PHP routing framework.",
	"keywords": [
		"php",
		"tiphy",
		"routing",
		"framework"
	],
	"minimum-stability": "stable",
	"prefer-stable": true,
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Ramelow",
			"email": "info@codekandis.net",
			"homepage": "https://codekandis.net",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@codekandis.net",
		"issues": "https://github.com/codekandis/tiphy/issues",
		"docs": "https://github.com/codekandis/tiphy/blob/master/README.md",
		"source": "https://github.com/codekandis/tiphy/tree/master"
	},
	"require": {
		"php": "^7.4",
		"codekandis/constants-classes-translator": "^1.0",
		"codekandis/json-codec": "^2",
		"codekandis/regular-expressions": "^0"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"codekandis/phpunit": "^3"
	},
	"autoload": {
		"psr-4": {
			"CodeKandis\\Tiphy\\": [
				"src/"
			]
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CodeKandis\\Tiphy\\Tests\\": [
				"tests/"
			]
		}
	},
	"scripts": {
		"post-update-cmd": [
		]
	}
}
